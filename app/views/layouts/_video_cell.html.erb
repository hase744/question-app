<div class="video_list_area">
    <div class="video_list_flame">
        <div class="youtube_video_area">
            <iframe class="youtube_video"src="https://www.youtube.com/embed/<%=content.youtube_id%>" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
        </div>
        <%= link_to(user_transaction_path(content.id), class: 'sellers') do %>
        <div class="video_text_area">
            <h4><%= content.title%></h4>
            <h5><%= content.seller.name%></h5>
            <h5 class="datetime"><%= from_now(content.created_at)%></h5>
        </div>
        <% end %>
    </div>
</div>
