<% if contents.present? %>
    <% contents.each do |user| %>
        <div class="followed_user_area">
            <%= link_to(user_account_path(user.id), class: 'seller_area') do %>
                <%=image_tag user.thumb_with_default, class:"seller_image"%>
                <div class="seller_name_area">
                    <p class="seller_name"><%=user.name%></p>
                </div>
            <% end %>
        </div>
    <% end %>
<% end %>