<%= render "/admin_user/shared/index", 
  	models: @coupons, 
  	column_names: ['user_id', 'usage_type', 'start_at', 'end_at', 'amount', 'discount_rate', 'minimum_purchase_amount'],
	show_exist: true,
	delete_exist: true,
	edit_exist: true,
	links: -> (model){[
		{
		text: "コピー", 
		link: new_admin_user_coupon_path(
				request.query_parameters.merge(
				coupon: model.slice(
					:amount, :discount_rate, :start_at, :end_at, :minimum_purchase_amount, :usage_type
					).merge(
						user_id: model.id
					)
				)
			)
		}
	]}
	%>